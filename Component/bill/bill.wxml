<!--Component/bill/bill.wxml-->
<view class="box">
		<view class="dingdanhao">订单号： {{dataD.orNumber}} <text style="color: #42b852;" catchtap="onfu">复制</text></view>
			<view class="flight1">
			<view style="width: 180rpx;text-align: center;">{{dataD.setout}}</view>
			<view class="feiji"><image class="fei" src="/assets/images/dafeiji.png"></image></view>
			<view style="width: 180rpx;text-align: center;">{{dataD.destination}}</view>
			</view>
		<view class="xx">{{dataD.status}}</view>
		<view class="boxflightext">
			<view class="flightext">订单详情</view>
			<view class="flightext1" wx:if="{{dataD.procedure == 2}}">确认打包</view>
			<view class="flightext1" wx:if="{{dataD.procedure == 1}}">付款</view>
			<view class="flightext1" wx:if="{{dataD.procedure == 0}}">物流详情</view>
		</view>
	</view>
